{
  "name": "openapi-parser",
  "version": "1.0.0",
  "main": "index.js",
  "private": "true",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "bun build src/index.ts --platform=node --outfile=dist/index.js",
    "start": "node dist/index.js",
    "dev": "bun src/index.ts --hot",
    "swagger": "swagger-codegen generate -i $npm_config_input -l html2 -o $npm_config_output"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.5",
    "bun": "^1.1.26",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@readme/openapi-parser": "^2.6.0",
    "dotenv": "^16.4.5",
    "yaml": "^2.5.0"
  }
}